{
    "manifest_version": 3,
    "name": "Snooze Mails Gmail Extension",
    "version": "1.0",
    "content_scripts": [
      {
        "matches": ["https://mail.google.com/*"],
        "js": ["content.js"]
      }
    ],
    "permissions": [
      "https://mail.google.com/",
      "https://www.googleapis.com/auth/gmail.modify"
    ],
    "icons":{
    },
    "background": {
      "service-worker":"background.js"
    }
  }
  